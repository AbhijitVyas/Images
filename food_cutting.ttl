#################################################################
#    Object Properties
#################################################################

###  http://www.ease-crc.org/ont/SOMA.owl#affordsTask
<http://www.ease-crc.org/ont/SOMA.owl#affordsTask> rdf:type owl:ObjectProperty ;
                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> .


###  http://www.ease-crc.org/ont/SOMA.owl#affordsTrigger
<http://www.ease-crc.org/ont/SOMA.owl#affordsTrigger> rdf:type owl:ObjectProperty .


###  http://www.ease-crc.org/ont/SOMA.owl#hasDisposition
<http://www.ease-crc.org/ont/SOMA.owl#hasDisposition> rdf:type owl:ObjectProperty .


###  http://www.ease-crc.org/ont/food_cutting#affordsPosition
:affordsPosition rdf:type owl:ObjectProperty .


###  http://www.ease-crc.org/ont/food_cutting#hasColor
:hasColor rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain <http://purl.obolibrary.org/obo/FOODON_00001057> ;
          rdfs:range :Color .


###  http://www.ease-crc.org/ont/food_cutting#hasEdibility
:hasEdibility rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :FoodPart ;
              rdfs:range :Edibility .


###  http://www.ease-crc.org/ont/food_cutting#hasInitialState
:hasInitialState rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasState ;
                 rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ;
                 rdfs:range :FruitState .


###  http://www.ease-crc.org/ont/food_cutting#hasInputObject
:hasInputObject rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.ease-crc.org/ont/food_cutting#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :PartInside ;
             rdfs:range :FoodPart .


###  http://www.ease-crc.org/ont/food_cutting#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         owl:inverseOf :partOf ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :Food ;
         rdfs:range :FoodPart .


###  http://www.ease-crc.org/ont/food_cutting#hasResultObject
:hasResultObject rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:range :Shape .


###  http://www.ease-crc.org/ont/food_cutting#hasResultingState
:hasResultingState rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasState ;
                   rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ;
                   rdfs:range :FruitState .


###  http://www.ease-crc.org/ont/food_cutting#hasSize
:hasSize rdf:type owl:ObjectProperty ;
         rdfs:domain <http://purl.obolibrary.org/obo/FOODON_00001057> ;
         rdfs:range :Size .


###  http://www.ease-crc.org/ont/food_cutting#hasState
:hasState rdf:type owl:ObjectProperty .


###  http://www.ease-crc.org/ont/food_cutting#partOf
:partOf rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty .


###  http://www.ease-crc.org/ont/food_cutting#requiresPriorTask
:requiresPriorTask rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.ease-crc.org/ont/meal_prep#hasPriorTask> ;
                   rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ;
                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> .


###  http://www.ease-crc.org/ont/food_cutting#target
:target rdf:type owl:ObjectProperty ;
        rdfs:domain :CuttingTask ;
        rdfs:range <http://purl.obolibrary.org/obo/FOODON_00001057> .


###  http://www.ease-crc.org/ont/meal_prep#hasPriorTask
<http://www.ease-crc.org/ont/meal_prep#hasPriorTask> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.ease-crc.org/ont/food_cutting#amount
:amount rdf:type owl:DatatypeProperty ;
        rdfs:domain :CuttingTask ;
        rdfs:range xsd:integer .


###  http://www.ease-crc.org/ont/food_cutting#angle
:angle rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:integer .


###  http://www.ease-crc.org/ont/food_cutting#genus
:genus rdf:type owl:DatatypeProperty ;
       rdfs:domain <http://purl.obolibrary.org/obo/FOODON_00001057> ;
       rdfs:range xsd:string ;
       rdfs:comment "describes the biological genus" .


###  http://www.ease-crc.org/ont/food_cutting#repetitions
:repetitions rdf:type owl:DatatypeProperty ;
             rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> .


