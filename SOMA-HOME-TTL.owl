###  http://www.ease-crc.org/ont/SOMA.owl#Disposition
SOMA:Disposition rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Extrinsic ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy> ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass SOMA:Affordance
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy> ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass [ owl:intersectionOf ( SOMA:Affordance
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesBearer ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesEventType ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#EventType>
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty SOMA:definesTrigger ;
                                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role>
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ]
                                 ] ;
                 rdfs:comment "The tendency of an object (the bearer) to make certain events happen with others (the triggers)." ,
                              "extrinsic" ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


