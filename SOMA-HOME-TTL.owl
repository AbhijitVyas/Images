                                                   rdfs:comment "A relationship indicating a Situation is prevented by another from manifesting." ;
                                                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                                                    <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                                                   rdfs:label "prevented by" .


###  http://www.ease-crc.org/ont/SOMA.owl/prevents
<http://www.ease-crc.org/ont/SOMA.owl/prevents> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor> ;
                                                rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
