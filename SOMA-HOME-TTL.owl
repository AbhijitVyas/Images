                         rdfs:comment "A task in which an Agent endeavors to describe truthfully some state of affairs." ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                          <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                         rdfs:label "Communication report"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationTask
SOMA:CommunicationTask rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task> ,
                                       [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Channel
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Message
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Receiver
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf> ;
                                                                owl:someValuesFrom SOMA:Sender
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                         owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                 owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                                               <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                                             )
                                                                               ]
                                                           ]
                                       ] ;
                       rdfs:comment """A Task in which two or more Agents exchange information. A CommunicationTask classifies only Events that have only Agents and Social objects as participants.

Of course, the means of exchange is Physical, however this concept is to classify events for which we are not interested in the physical substrate, but rather who communicated and what the information content was.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                       rdfs:label "Communication task"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CommunicationTopic
SOMA:CommunicationTopic rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:ResourceRole ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                          owl:allValuesFrom [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject>
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn> ;
                                                                                     owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy> ;
                                                                                                          owl:someValuesFrom SOMA:CommunicationTask
                                                                                                        ]
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                        ] ;
                        rdfs:comment """A role that appears in communication tasks, and indicates what the communication is about.

CommunicationTopic can only classify a Social object that participates in an Action that is classified as (the execution of) a CommunicationTask.

Note that classifies here is used in the plays-role sense. This isn't to say that the social object, ie the information exchanged in the communication, is an instance of the topic. Rather, the topic role refers to what the information is about.

For example, if the topic of a communication is flowers, this does not mean the words themselves are flowers, merely that, in some sense, they are about flowers.""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                         <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                        rdfs:label "Communication topic"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Compartment
SOMA:Compartment rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:DesignedComponent ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Composing
SOMA:Composing rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Variability ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty SOMA:affordsTrigger ;
                                 owl:allValuesFrom SOMA:ConnectedObject
                               ] ;
               rdfs:comment "The disposition of an object (the tool) to change the compositional structure of others." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Computer_Language
SOMA:Computer_Language rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:FormalLanguage ;
                       rdfs:comment """A computer language is a formal language used in communication with a computer.

From Wikipedia: https://en.wikipedia.org/wiki/Computer_language"""@en ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                       rdfs:label "Computer Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Computer_Program
SOMA:Computer_Program rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( SOMA:Structured_Text
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty SOMA:isGivenMeaningBy ;
                                                                   owl:someValuesFrom SOMA:Programming_Language
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ,
                                          [ owl:intersectionOf ( SOMA:Structured_Text
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses> ;
                                                                   owl:someValuesFrom SOMA:Algorithm
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:comment "The Program itself (the specific set of instruction in a Programming Language), not the file that it is contained in nor the implemented algorithm!"@en ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                       <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                      rdfs:label "Computer program"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Conclusion
SOMA:Conclusion rdf:type owl:Class ;
                rdfs:subClassOf SOMA:CreatedObject ,
                                SOMA:Knowledge ;
                rdfs:comment "An object that is derived from some premise using some inference rules." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                rdfs:label "Conclusions" .


###  http://www.ease-crc.org/ont/SOMA.owl#ConditionalSuccedence
SOMA:ConditionalSuccedence rdf:type owl:Class ;
                           rdfs:subClassOf SOMA:Succedence ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty SOMA:hasBinding ;
                                             owl:allValuesFrom SOMA:CounterfactualBinding
                                           ] ;
                           rdfs:comment """A relation that holds between two OGPTasks that belong to the same OGPWorkflow, and which means that, if a condition is met, the successor task invocation is to be executed after the predecessor task invocation completes.

The condition is a conjunction of CounterfactualBindings. These bindings may be RoleRoleBindings (meaning, test whether the fillers for these Roles/Parameters are the same) or RoleFillerBindings (meaning, test whether the filler of the Role unifies with the candidate Entity).

An empty conjunction of CounterfactualBindings is assumed to be True.""" ,
                                        "Note: in RobotWorkflows, Tasks typically may have several possible successors, to be selected based on condition-testing." ;
                           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                            <http://www.ease-crc.org/ont/SOMA-WF.owl> ;
                           rdfs:label "Conditional succedence"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Configuration
SOMA:Configuration rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes> ;
                                     owl:someValuesFrom SOMA:StateType
                                   ] ;
                   rdfs:comment """A description of a State. This includes e.g. what acceptable regions for participant objects of the State might look like.
A configuration of the world is construed to be stable on its own. Outside disturbances may cause state transitions, and the settling into some other, self-stable configuration.

Several other Description subclasses may function as Configurations. For example, a Goal is a description of a desired State. A Norm describes a State that should be maintained. A Diagnosis describes a State that causes certain symptoms etc.

Note a couple of issues here. First one relates to what \"configuration\" means; in particular, this doesn't mean a configuration that is unchanging according to any viewpoint. The analogy here is the \"macrostate\" from thermodynamics: a macrostate with two gases mixed does not mean all gas particles are motionless, but rather that the locations and movements of gas particles are such that any particle is likely to have as many neighbors of one type as the other.

The second issue relates to what is \"outside\". The state is a configuration of some, but not necessarily all, Entities in the world. Entities not in this configuration are outside of it.""" ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ConnectedObject
SOMA:ConnectedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object that is combined with another object." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                     rdfs:label "Connected object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Connectivity
SOMA:Connectivity rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Disposition ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty SOMA:affordsBearer ;
                                    owl:allValuesFrom SOMA:ConnectedObject
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty SOMA:affordsTrigger ;
                                    owl:allValuesFrom SOMA:ConnectedObject
                                  ] ;
                  rdfs:comment "The disposition of an object (the connected object) to establish a connection with others." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Constructing
SOMA:Constructing rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:PhysicalTask ;
                  owl:disjointWith SOMA:ModifyingPhysicalObject ;
                  rdfs:comment "A task in which an Agent creates a new physical object."@en ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ContactState
SOMA:ContactState rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:StateType ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                    owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                                      ]
                                  ] ;
                  rdfs:comment "Classifies States in which some objects are in contact." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                  rdfs:label "Contact state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Container
SOMA:Container rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Restrictor ;
               rdfs:comment "A role classifying an object used to contain others." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Containment
SOMA:Containment rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Disposition ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsBearer ;
                                   owl:allValuesFrom SOMA:Container
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsTrigger ;
                                   owl:allValuesFrom SOMA:IncludedObject
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:isDispositionOf ;
                                   owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
                                                       owl:someValuesFrom SOMA:Capacity
                                                     ]
                                 ] ;
                 rdfs:comment "Classifies States in which an object is placed inside another." ,
                              "The disposition of an object (the container) to contain others." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#ContainmentState
SOMA:ContainmentState rdf:type owl:Class ;
                      rdfs:subClassOf SOMA:FunctionalControl ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty SOMA:isStateTypeOf ;
                                        owl:someValuesFrom [ owl:intersectionOf ( SOMA:Container
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                                    owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                                                                        owl:onProperty SOMA:hasDisposition ;
                                                                                                        owl:someValuesFrom SOMA:Containment
                                                                                                      ]
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                      ] ;
                      rdfs:comment "Classifies States in which an object is kept inside another object." ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                       <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                      rdfs:label "Containment state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ContainmentTheory
SOMA:ContainmentTheory rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:ControlTheory ;
                       rdfs:comment """A schematic theory that describes a functional relation which ensures that the location of some entity, the locatum, is constrained to be within some region which is the interior of some other entity, the relatum.

This is also known as FunctionalControlInternal in GUM-4-space (Bateman et al. 2010).

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL isInsideOf xR.""" ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Containment theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#ContinuousJoint
SOMA:ContinuousJoint rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:HingeJoint ;
                     rdfs:comment "A continuous hinge joint that rotates around an axis and has no upper and lower limits." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Continuous joint" .


###  http://www.ease-crc.org/ont/SOMA.owl#ControlTheory
SOMA:ControlTheory rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:FunctionalSpatialSchemaTheory ;
                   rdfs:comment """A description of functional-spatial configurations where one object controls another object's position in space, e.g. if a pot moves, then the popcorn contained therein moves, as well. Note that the objects do not need to be in direct contact.

Adopted from GUM-4-space (Bateman et al. 2010).""" ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                   rdfs:label "Control theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Cooktop
SOMA:Cooktop rdf:type owl:Class ;
             rdfs:subClassOf SOMA:DesignedComponent ;
             rdfs:comment "A typically circular surface where food is cooked." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CounterfactualBinding
SOMA:CounterfactualBinding rdf:type owl:Class ;
                           rdfs:subClassOf SOMA:Binding ;
                           owl:disjointWith SOMA:FactualBinding ;
                           rdfs:comment """CounterfactualBindings are used to express conditions:

-- that two roles share a filler (RoleRoleBindings);
-- that the filler of a role is a particular entity (RoleFillerBindings). This is typically the case when the filler of the role may be one of a few constants, as is the case with the execution status of a robot task.

TODO: for the robot workflows we are likely to need at the start, testing equality of fillers is enough. In the future, we may want to replace that with unifiability of fillers.""" ;
                           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                            <http://www.ease-crc.org/ont/SOMA-WF.owl> ;
                           rdfs:label "Counterfactual binding"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Countertop
SOMA:Countertop rdf:type owl:Class ;
                rdfs:subClassOf SOMA:DesignedComponent ;
                rdfs:comment "A flat surface for working on." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cover
SOMA:Cover rdf:type owl:Class ;
           rdfs:subClassOf SOMA:Barrier ;
           rdfs:comment "An object used to cover up others, such as a lid used as a cover for a pot." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                            <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Coverage
SOMA:Coverage rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Blockage ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:affordsBearer ;
                                owl:allValuesFrom SOMA:Cover
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:affordsTrigger ;
                                owl:allValuesFrom SOMA:CoveredObject
                              ] ;
              rdfs:comment "The disposition of an object (the cover) to hide or to protect objects by covering them. An example is a door that covers items in a container to e.g. prevent dust getting inside of the container." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CoverageTheory
SOMA:CoverageTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:FunctionalSpatialSchemaTheory ;
                    rdfs:comment """A schematic theory of a functional relation between two objects such that one of them, the locatum, blocks access to the interior of the relatum.

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL coversObject xR.""" ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                     <http://www.ease-crc.org/ont/SOMA-SAY.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CoveredObject
SOMA:CoveredObject rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:BlockedObject ;
                   rdfs:comment "An object that is covered." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Covered object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CoveringTheory
SOMA:CoveringTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:ExecutableSchematicTheory ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Instrument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass SOMA:Patient
                                    ] ;
                    rdfs:comment "A schematic theory of how an agent can use an instrument to prevent access to the interior of a patient." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                     <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                    rdfs:label "Covering theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CrackingTheory
SOMA:CrackingTheory rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:ExecutableSchematicTheory ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Instrument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                      owl:someValuesFrom SOMA:Patient
                                    ] ;
                    rdfs:comment "A schematic theory of how an agent can inflict damage to the surface of an object." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                     <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                    rdfs:label "Cracking theory"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#CreatedObject
SOMA:CreatedObject rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:Patient ;
                   rdfs:comment "An object that is created." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                   rdfs:label "Created object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Creation
SOMA:Creation rdf:type owl:Class ;
              rdfs:subClassOf SOMA:ProcessType ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:isProcessTypeOf ;
                                owl:someValuesFrom SOMA:CreatedObject
                              ] ;
              rdfs:comment """A process by which an Entity is created in the physical world.

Note, most of the physical Entities we will be considering here are in fact arrangements of many other, smaller physical Entities. Therefore another way to look at this is, a 'Physical creation' is the process by which a set of physical Entities is arranged in a certain way, and the arrangement is then itself considered a physical Entity.""" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
              rdfs:label "Creation" .


###  http://www.ease-crc.org/ont/SOMA.owl#Crockery
SOMA:Crockery rdf:type owl:Class ;
              rdfs:subClassOf SOMA:DesignedContainer ,
                              SOMA:Tableware ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasDisposition ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass SOMA:Insertion
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasDisposition ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass [ owl:intersectionOf ( SOMA:Insertion
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty SOMA:affordsTrigger ;
                                                                     owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                                         owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Substance>
                                                                                       ]
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ]
                              ] ;
              rdfs:comment "An object designed to contain food to store, cook, eat, or serve it." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cup
SOMA:Cup rdf:type owl:Class ;
         rdfs:subClassOf SOMA:Crockery ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty SOMA:hasPhysicalComponent ;
                           owl:someValuesFrom SOMA:DesignedHandle
                         ] ;
         rdfs:comment "A round shaped container used for storing liquid and drinking out of it, typically has  a handle for grasping." ;
         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cupboard
SOMA:Cupboard rdf:type owl:Class ;
              rdfs:subClassOf SOMA:DesignedContainer ,
                              SOMA:DesignedFurniture ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:hasPhysicalComponent ;
                                owl:someValuesFrom SOMA:Rack
                              ] ;
              rdfs:comment "A piece of furniture for storing things." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CutObject
SOMA:CutObject rdf:type owl:Class ;
               rdfs:subClassOf SOMA:AlteredObject ;
               rdfs:comment "An object being cut down into pieces." ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
               rdfs:label "Cut object" .


###  http://www.ease-crc.org/ont/SOMA.owl#Cutlery
SOMA:Cutlery rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Tableware ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cuttability
SOMA:Cuttability rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Disposition ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsBearer ;
                                   owl:allValuesFrom SOMA:CutObject
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:affordsTrigger ;
                                   owl:allValuesFrom SOMA:Cutter
                                 ] ;
                 rdfs:comment "The disposition of an object (the barrier) which makes it able to be cut down (into pieces) usually by some other object with role as a Cutter. Such as a cucumber has cuttability disposition which can be cut by a knife." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cutter
SOMA:Cutter rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Tool ;
            rdfs:comment "A role to classify an object used to cut other objects. Usually should poses sharpness as a quality. Execptions are not considered in this context. Such as a wind, water, or other natural agents cutting(eroding) the rocks." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                             <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Cutting
SOMA:Cutting rdf:type owl:Class ;
             rdfs:subClassOf SOMA:ModifyingPhysicalObject ;
             rdfs:comment "The goal of this task is to separate one or more pieces from some target object by means of cutting into its constituent material. Unlike a disassembly, a cut is usually not easily reversible." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                              <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CuttingTool
SOMA:CuttingTool rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:DesignedTool ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:hasDisposition ;
                                   owl:someValuesFrom SOMA:Shaping
                                 ] ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#CylinderShape
SOMA:CylinderShape rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:ShapeRegion ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasRadius ;
                                     owl:someValuesFrom xsd:float
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasLength ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:float
                                   ] ;
                   rdfs:comment "A solid geometrical figure with straight parallel sides and a circular or oval cross section." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                   rdfs:label "Cylinder shape" .


###  http://www.ease-crc.org/ont/SOMA.owl#Database
SOMA:Database rdf:type owl:Class ;
              rdfs:subClassOf SOMA:SoftwareRole ;
              rdfs:comment """A Database is a Software that organizes a collection of data stored and and allows for access, usually via some query engine.

Source: https://en.wikipedia.org/wiki/Database"""@en ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-IO.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Deciding
SOMA:Deciding rdf:type owl:Class ;
              rdfs:subClassOf SOMA:DerivingInformation ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               "A mental task in where an agent makes some decision, that is, selecting some Information object; usually on the basis of others."@en ;
              rdfs:label "Deciding"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#DeclarativeClause
SOMA:DeclarativeClause rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:ClausalObject ;
                       rdfs:comment "A clause which makes an assertion or declaration." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                       rdfs:label "Declarative clause" .


###  http://www.ease-crc.org/ont/SOMA.owl#DeductiveReasoning
SOMA:DeductiveReasoning rdf:type owl:Class ;
                        rdfs:subClassOf SOMA:Reasoning ;
                        rdfs:comment """A task in which the Agent, using general logical principles that it considers logically valid, applied to premises that it considers logically true, arrives at conclusions that it considers logically certain.

Deduction is often explained as starting from the \"general\" (some property X is known about all members of a set S), applying it to the \"specific\" (some Entity Y is known to belong to set S), to arrive at a specialization of the general property (X applies to Y).""" ;
                        rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                         <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                        rdfs:label "Deductive reasoning"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deformation
SOMA:Deformation rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:Alteration ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty SOMA:isProcessTypeOf ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass SOMA:ShapedObject
                                 ] ;
                 owl:disjointWith SOMA:FluidFlow ;
                 rdfs:comment """A process by which a physical Entity changes its shape under the influence of some factors outside of the Entity.

Note, changes of shape may be self-caused; for example, a gas will naturally disperse. This however would be a different type of process (Dispersion).

A soft slab of clay deforming under its own weight on Earth would still count as deformation: it is the gravity of the Earth (so, a factor outside the slab of clay) which makes the slab change shape.""" ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Delivering
SOMA:Delivering rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Actuating ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:isTaskAffordedBy ;
                                  owl:someValuesFrom SOMA:Shifting
                                ] ;
                owl:disjointWith SOMA:Fetching ,
                                 SOMA:Lifting ,
                                 SOMA:Opening ,
                                 SOMA:Pulling ,
                                 SOMA:Pushing ,
                                 SOMA:Squeezing ;
                rdfs:comment "A task in which an Agent brings an item that the Agent already carries to a specified target." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DependentPlace
SOMA:DependentPlace rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Feature ;
                    rdfs:comment "A feature that is not part of its host, like a hole in a piece of cheese." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                     <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                    rdfs:label "Dependent place"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deposit
SOMA:Deposit rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Instrument ;
             rdfs:comment "A role classifying an object ontop which others are put to e.g. store them, or to place them in a meaningful way for future activities." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                              <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DepositedObject
SOMA:DepositedObject rdf:type owl:Class ;
                     rdfs:subClassOf SOMA:Patient ;
                     rdfs:comment "An object placed ontop of another one." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                     rdfs:label "Deposited object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Deposition
SOMA:Deposition rdf:type owl:Class ;
                rdfs:subClassOf SOMA:Disposition ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsBearer ;
                                  owl:allValuesFrom SOMA:Deposit
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:affordsTrigger ;
                                  owl:allValuesFrom SOMA:DepositedObject
                                ] ;
                rdfs:comment "The disposition to support objects." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DerivingInformation
SOMA:DerivingInformation rdf:type owl:Class ;
                         rdfs:subClassOf SOMA:InformationAcquisition ,
                                         [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty SOMA:isTaskOfInputRole ;
                                                                  owl:someValuesFrom SOMA:Premise
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty SOMA:isTaskOfOutputRole ;
                                                                  owl:someValuesFrom SOMA:Conclusion
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                          <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                         rdfs:label "Deriving information" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedChair
SOMA:DesignedChair rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:DesignedFurniture ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty SOMA:hasDisposition ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass SOMA:CanBeSatOn
                                   ] ;
                   rdfs:comment "A piece of furniture designed for one person to sit on." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedComponent
SOMA:DesignedComponent rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:FunctionalPart ,
                                       <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:hasDisposition ;
                                         owl:someValuesFrom SOMA:Connectivity
                                       ] ;
                       rdfs:comment "An object designed to be part or element of a larger whole." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed component" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedContainer
SOMA:DesignedContainer rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty SOMA:hasDisposition ;
                                         owl:someValuesFrom SOMA:Containment
                                       ] ;
                       rdfs:comment "An item designed to be able to hold some other items, preventing their free movement and/or protecting them from outside influence. Containers may be used for storage, or to obtain control over items that are otherwise hard to manipulate directly (e.g. liquids)." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed container" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedFurniture
SOMA:DesignedFurniture rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ;
                       rdfs:comment "An object used to make a room or building suitable for living or working." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                       rdfs:label "Designed furniture" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedHandle
SOMA:DesignedHandle rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:DesignedComponent ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty SOMA:hasDisposition ;
                                      owl:someValuesFrom SOMA:Graspability
                                    ] ;
                    rdfs:comment "An item designed to fit well within a grasping hand, often attached to another item to enhance its manipulability." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                    rdfs:label "Designed handle" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedSpade
SOMA:DesignedSpade rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:DesignedComponent ;
                   rdfs:comment "A sharp-edged metal blade used for digging or cutting." ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                   rdfs:label "Designed spade" .


###  http://www.ease-crc.org/ont/SOMA.owl#DesignedTool
SOMA:DesignedTool rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ;
                  rdfs:comment "An item designed to enable some action, in which it will play an instrumental role." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-IO.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                  rdfs:label "Designed tool" .


###  http://www.ease-crc.org/ont/SOMA.owl#DessertFork
