                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                  rdfs:label "Instructions" .


###  http://www.ease-crc.org/ont/SOMA.owl#Instrument
SOMA:Instrument rdf:type owl:Class ;
                rdfs:subClassOf SOMA:ResourceRole ;
                rdfs:comment "An object used to carry out the event." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-SAY.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#InterfaceComponentRole
SOMA:InterfaceComponentRole rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( SOMA:SoftwareRole
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn> ;
                                                                         owl:allValuesFrom SOMA:InterfaceSpecification
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf SOMA:SoftwareRole ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                             <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                            rdfs:label "Interface component role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InterfaceSpecification
SOMA:InterfaceSpecification rdf:type owl:Class ;
                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Design> ;
                            rdfs:comment """The Specification of an Interface between software, computer hardware, peripheral devices, humans and combinations of these.

Source: https://en.wikipedia.org/wiki/Interface_(computing)"""@en ;
                            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                             <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                            rdfs:label "Interface Specification"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#IntermediateGrasp
SOMA:IntermediateGrasp rdf:type owl:Class ;
                       rdfs:subClassOf SOMA:GraspingMotion ;
                       owl:disjointWith SOMA:PowerGrasp ,
                                        SOMA:PrecisionGrasp ;
                       rdfs:comment "A kind of grasp that acquires kinematic control over the gripped object, but without attempting to achieve either strong holding force nor precision of subsequent movement of the gripped object." ;
                       rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                        <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                       rdfs:label "Intermediate grasp" .


###  http://www.ease-crc.org/ont/SOMA.owl#Interpreting
SOMA:Interpreting rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:DerivingInformation ;
                  rdfs:comment "A task in which an Agent interpretes some information, e.g., makes sense of some incoming message or its visible surroundings."@en ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                  rdfs:label "Interpreting"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#InterrogativeClause
SOMA:InterrogativeClause rdf:type owl:Class ;
                         rdfs:subClassOf SOMA:ClausalObject ;
                         rdfs:comment """A clause which makes a request, typically information, of some agent.

Note that in a semantic sense such clauses always request information, but in a pragmatic sense they can be used to convey commands or requests for action, such as e.g. \"can you close the door?\" The question is not just a request for information about ability, but a request to perform a task.""" ;
                         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                          <http://www.ease-crc.org/ont/SOMA-SAY.owl> ;
                         rdfs:label "Interrogative clause" .


###  http://www.ease-crc.org/ont/SOMA.owl#Intrinsic
SOMA:Intrinsic rdf:type owl:Class ;
               rdfs:subClassOf SOMA:PhysicalQuality ;
               rdfs:comment "A physical quality that is independent of context." ,
                            "intrinsic" ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Introspecting
SOMA:Introspecting rdf:type owl:Class ;
                   rdfs:subClassOf SOMA:InformationAcquisition ;
                   rdfs:comment "A mentalk task in which an Agent gathers and processes information about its own mental tasks via, e.g., Meta Reasoning."@en ;
                   rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                    <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
                   rdfs:label "Introspecting"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Item
SOMA:Item rdf:type owl:Class ;
          rdfs:subClassOf SOMA:Patient ;
          rdfs:comment "A role played by a non-agentive object operated on by an action." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-STATE.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#JamJar
SOMA:JamJar rdf:type owl:Class ;
            rdfs:subClassOf SOMA:Jar ;
            rdfs:comment "A jar which holds, or at least is intended to usually hold, jam." ;
            rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Jar
SOMA:Jar rdf:type owl:Class ;
         rdfs:subClassOf SOMA:DesignedContainer ;
         rdfs:comment "A container used for long-term storage of some liquid or viscous dish." ;
         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Joint
SOMA:Joint rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( SOMA:FixedJoint
                                               SOMA:MovableJoint
                                             )
                               ] ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalBody> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasChildLink ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasParentLink ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasJointState ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass SOMA:JointState
                           ] ;
           rdfs:comment "An object that is used to articulate links in a kinematic structure." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                            <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#JointLimit
SOMA:JointLimit rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ;
                rdfs:comment "The physical limits of a joint." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Joint limit" .


###  http://www.ease-crc.org/ont/SOMA.owl#JointState
SOMA:JointState rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointPosition ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointVelocity ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:hasJointEffort ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:double
                                ] ;
                rdfs:comment "The state of a joint in terms of position, velocity of the joint and effort applied to it." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                rdfs:label "Joint state" .


###  http://www.ease-crc.org/ont/SOMA.owl#KineticFrictionAttribute
SOMA:KineticFrictionAttribute rdf:type owl:Class ;
                              rdfs:subClassOf SOMA:FrictionAttribute ;
                              rdfs:comment "Friction that occurs when two touching objects are moving relative to each other." ;
                              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                               <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                              rdfs:label "Kinetic friction attribute" .


###  http://www.ease-crc.org/ont/SOMA.owl#KinoDynamicData
SOMA:KinoDynamicData rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout> ;
                                       owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                     ] ;
                     rdfs:comment "An InformationObject containing data about how a physical object is put together such that its parts may move relative to each other, and what the physical characteristics of those parts are." ;
                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                      <http://www.ease-crc.org/ont/SOMA-NEEM.owl> ;
                     rdfs:label "Kino-dynamic data"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Kitchen
SOMA:Kitchen rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Room ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty SOMA:hasDisposition ;
                               owl:someValuesFrom SOMA:Containment
                             ] ;
             rdfs:comment "A designated room to perform the task of cooking." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#KitchenCabinet
SOMA:KitchenCabinet rdf:type owl:Class ;
                    rdfs:subClassOf SOMA:Cupboard ;
                    rdfs:comment "A cupboard designed to be used in kitchen environments." ;
                    rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#KitchenKnife
SOMA:KitchenKnife rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Knife ;
                  rdfs:comment "A tool designed to be used for kitchen related common tasks. Such as smearing a bread or cutting a cucumber." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#KitchenUnit
SOMA:KitchenUnit rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:DesignedFurniture ;
                 rdfs:comment "A piece of a fitted furniture typically containing cupboards, a sink, and an oven." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Knife
SOMA:Knife rdf:type owl:Class ;
           rdfs:subClassOf SOMA:CuttingTool ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasPhysicalComponent ;
                             owl:someValuesFrom SOMA:Blade
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty SOMA:hasPhysicalComponent ;
                             owl:someValuesFrom SOMA:DesignedHandle
                           ] ;
           rdfs:comment "An instrument composed of a blade fixed into a handle, used for cutting or as a weapon." ;
           rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Knowledge
SOMA:Knowledge rdf:type owl:Class ;
               rdfs:subClassOf SOMA:Item ;
               rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#KnowledgeRepresentationLanguage
SOMA:KnowledgeRepresentationLanguage rdf:type owl:Class ;
                                     rdfs:subClassOf SOMA:Computer_Language ;
                                     rdfs:comment "A Knowledge Representation Language is a Language with fixed semantics and syntax to describe some knowledge. Examples are JSON and the different OWL Profiles."@en ;
                                     rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                                      <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
                                     rdfs:label "Knowledge Representation Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Labeling
SOMA:Labeling rdf:type owl:Class ;
              rdfs:subClassOf SOMA:Interpreting ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
              rdfs:label "A task in which an agent annotates some objects with names, e.g., labeling the feeling of a tip-of-the-tongue event as such." ,
                         "Labeling" .


###  http://www.ease-crc.org/ont/SOMA.owl#Language
SOMA:Language rdf:type owl:Class ;
              rdfs:subClassOf SOMA:System ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty SOMA:givesMeaningTo ;
                                owl:allValuesFrom SOMA:Text
                              ] ;
              rdfs:comment """A Language is a structured System for communication.

From Wikipedia: https://en.wikipedia.org/wiki/Language"""@en ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-IO.owl> ;
              rdfs:label "Language"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Leaning
SOMA:Leaning rdf:type owl:Class ;
             rdfs:subClassOf SOMA:PosturalMoving ;
             rdfs:comment "An Agent pitches its body in some direction." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                              <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Learning
SOMA:Learning rdf:type owl:Class ;
              rdfs:subClassOf SOMA:InformationStorage ;
              rdfs:comment """The mental task of storing information for later use.

This is more general than memorizing, as the later only captures declarative knowledge.""" ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-HOME.owl> ;
              rdfs:label "Learning" .


###  http://www.ease-crc.org/ont/SOMA.owl#Leg
SOMA:Leg rdf:type owl:Class ;
         rdfs:subClassOf SOMA:Limb ;
         rdfs:comment "A limb on which an agent walks or stands." ;
         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                          <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Lid
SOMA:Lid rdf:type owl:Class ;
         rdfs:subClassOf SOMA:DesignedComponent ;
         rdfs:comment "A removable or hinged cover for the top of a container." ;
         rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Lifting
SOMA:Lifting rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Actuating ;
             owl:disjointWith SOMA:Opening ,
                              SOMA:Pulling ,
                              SOMA:Pushing ,
                              SOMA:Squeezing ;
             rdfs:comment "todo: how to distinguish from e.g. 'pushing from the table'" ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                              <http://www.ease-crc.org/ont/SOMA-HOME.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Limb
SOMA:Limb rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( SOMA:Arm
                                              SOMA:Leg
                                            )
                              ] ;
          rdfs:subClassOf SOMA:PhysicalEffector ;
          rdfs:comment "An arm or leg of an embodied agent." ;
          rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                           <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LimbMotion
SOMA:LimbMotion rdf:type owl:Class ;
                rdfs:subClassOf SOMA:DirectedMotion ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty SOMA:isProcessTypeOf ;
                                  owl:someValuesFrom [ owl:intersectionOf ( SOMA:MovedObject
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                                                              owl:allValuesFrom SOMA:Limb
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
                owl:disjointWith SOMA:Locomotion ,
                                 SOMA:PrehensileMotion ;
                rdfs:comment "An Agent moves its limbs in some way." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-PROC.owl> ;
                rdfs:label "Limb motion" .


###  http://www.ease-crc.org/ont/SOMA.owl#LinguisticObject
SOMA:LinguisticObject rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
                      rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-ACT.owl> ,
                                       <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                       <http://www.ease-crc.org/ont/SOMA-SAY.owl> ,
                                       <http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl> ;
                      rdfs:label "Linguistic object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Linkage
SOMA:Linkage rdf:type owl:Class ;
             rdfs:subClassOf SOMA:Connectivity ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty SOMA:affordsBearer ;
                               owl:allValuesFrom SOMA:LinkedObject
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty SOMA:affordsTrigger ;
                               owl:allValuesFrom SOMA:LinkedObject
                             ] ;
             rdfs:comment "The disposition of an object (the linked object) to establish a connection with others by being linked together." ;
             rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                              <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LinkageState
SOMA:LinkageState rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:StateType ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty SOMA:isStateTypeOf ;
                                    owl:someValuesFrom SOMA:LinkedObject
                                  ] ;
                  rdfs:comment "Classifies States in which two objects are in a rigid connection, such that the movement of one determines the movement of the other." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                  rdfs:label "Linkage state"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#LinkedObject
SOMA:LinkedObject rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:ConnectedObject ;
                  rdfs:comment "An object that is linked to some other object." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-STATE.owl> ;
                  rdfs:label "Linked object"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Localization
SOMA:Localization rdf:type owl:Class ;
                  rdfs:subClassOf SOMA:Extrinsic ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion> ;
                                    owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion>
                                  ] ;
                  rdfs:comment "The localization of an object. The region of this quality encodes values to localize the object in a dimensional space, e.g. Euclidean positions that localize the object in Euclidean space." ;
                  rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                   <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#Location
SOMA:Location rdf:type owl:Class ;
              rdfs:subClassOf SOMA:SpatioTemporalRole ;
              rdfs:comment "A role classifying a location of interest, often specified as a spatial relation between several objects, themselves usually classified by spatial relation roles." ;
              rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                               <http://www.ease-crc.org/ont/SOMA-OBJ.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LocatumRole
SOMA:LocatumRole rdf:type owl:Class ;
                 rdfs:subClassOf SOMA:SpatialRelationRole ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ;
                                   owl:allValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                 ] ;
                 rdfs:comment "Denotes the object with primary focal prominence in a spatial or spatio-temporal schema. Terminological variants that appear in the literature on cognitive linguistics include Figure (Talmy 1983) and Trajector (Langacker 1986)." ;
                 rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                  <http://www.ease-crc.org/ont/SOMA-OBJ.owl> ;
                 rdfs:label "Locatum role"@en .


###  http://www.ease-crc.org/ont/SOMA.owl#Locomotion
SOMA:Locomotion rdf:type owl:Class ;
                rdfs:subClassOf SOMA:BodyMovement ,
                                SOMA:DirectedMotion ;
                owl:disjointWith SOMA:PrehensileMotion ;
                rdfs:comment """Conceptually related to Navigation, but distinguishable from it because of the profile, ie. the focus of the task.

Navigation is about reaching some goal.

Locomotion is concerned more with the actual motion.""" ,
                             "The Agent repositions their body in the environment." ;
                rdfs:isDefinedBy <http://www.ease-crc.org/ont/SOMA-HOME.owl> ,
                                 <http://www.ease-crc.org/ont/SOMA-PROC.owl> .


###  http://www.ease-crc.org/ont/SOMA.owl#LocutionaryAction
SOMA:LocutionaryAction rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( SOMA:CommunicationAction
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                                                    owl:someValuesFrom <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent>
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment """A Locutionary Act is the performance of an utterance (source: https://en.wikipedia.org/wiki/Locutionary_act).

We additionally require a Locutionary Act to be performed by an Agent, not an Actor - this is what sets it apart from a Communication Action."""@en ;
